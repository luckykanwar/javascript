# Looping a triangle
let strVal = "#";
let counter = 0;
while(counter<7){
	console.log(strVal);
	strVal = strVal + "#";
	counter += 1;
}


# FizzBuzz using if else
let counter = 1;
while(counter <= 100){
  if((counter % 3 == 0) && (counter % 5 == 0)){
    console.log("FizzBuzz");
  }else if (counter % 3 == 0){
    console.log("Fizz");
  }else if (counter % 5 == 0){
    console.log("Buzz");
  }else{
    console.log(counter);
  }
  counter += 1;
}
